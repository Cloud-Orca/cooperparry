    <formulas>
        <name>cpOffice_formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId},3) = &apos;001&apos;, TEXT({!get_Account.CP_Office__c}), TEXT({!get_Lead.CP_Office__c}))</expression>
    </formulas>
    <formulas>
        <name>estimatedFees_formula</name>
        <dataType>Currency</dataType>
        <expression>IF(LEFT({!recordId},3) = &apos;001&apos;, {!get_Account.Estimated_Fees__c}, {!get_Lead.Estimated_Fees__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>MeetingName</name>
        <dataType>String</dataType>
        <expression>TEXT(DATEVALUE({!Meeting_Date})) + &quot; meeting with &quot; + {!get_Lead.Company}</expression>
    </formulas>
    <formulas>
        <name>notes_formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId},3) = &apos;001&apos;, {!get_Account.Notes__c},{!get_Lead.Notes__c})</expression>
    </formulas>
    <formulas>
        <name>NoteTitle</name>
        <dataType>String</dataType>
        <expression>{!get_Lead.Company} + &quot; - &quot; + TEXT(TODAY()) + &quot; - Handover Notes&quot;</expression>
    </formulas>
    <formulas>
        <name>targetName_formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId},3)= &apos;001&apos;, {!get_Account.Name},{!get_Lead.Company})</expression>
    </formulas>
