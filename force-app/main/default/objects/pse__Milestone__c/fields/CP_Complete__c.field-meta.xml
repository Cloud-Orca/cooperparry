<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CP_Complete__c</fullName>
    <externalId>false</externalId>
    <formula>IF ( (  pse__Milestone_Amount__c   ==0 ||  
CT_Manual_Recovery_Rate__c == 0 || 
ISNULL(CT_Manual_Recovery_Rate__c)),0,((CT_Total_Submitted_Hours_Bill_Rate__c +  pse__Billable_Expenses_In_Financials__c 
 +  pse__Non_Billable_Expenses_In_Financials__c )/(pse__Milestone_Amount__c  / CT_Manual_Recovery_Rate__c )))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CP % Complete</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
